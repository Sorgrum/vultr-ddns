{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild:ui": "npx rimraf .next",
    "build:ui": "next build",
    "prebuild:server": "npx rimraf syncServer/dist",
    "build:server": "npm run --prefix syncServer build",
    "build": "concurrently -c \"black.bgBlue,green.bgBlack\" \"npm:build:ui\" \"npm:build:server\"",
    "start:ui": "next start",
    "start:server": "npm run --prefix syncServer start",
    "start": "concurrently -c \"black.bgBlue,green.bgBlack\" \"npm:start:ui\" \"npm:start:server\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/react": "^2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.0.0",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "await-to-js": "^3.0.0",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "express": "^4.18.2",
    "framer-motion": "^11.3.24",
    "i": "^0.3.7",
    "next": "13.2.4",
    "node-cron": "^3.0.2",
    "node-persist": "^4.0.3",
    "npm": "^9.6.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-toastify": "^9.1.2",
    "react-useinterval": "^1.0.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "@types/node-persist": "^3.1.8",
    "typescript": "^5.0.3"
  }
}
